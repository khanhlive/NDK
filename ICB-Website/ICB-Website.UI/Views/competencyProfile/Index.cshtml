@model ICB.Business.Models.SystemConfig
@{
    ViewBag.Title = "Hồ sơ năng lực";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
    ViewBag.NotRenderSideBar = true;
    List<ICB_Website.UI.Models.Breadcrumb> list = new List<ICB_Website.UI.Models.Breadcrumb>();
    list.Add(new ICB_Website.UI.Models.Breadcrumb { ID = 1, Name = ICB_Website.UI.Resources.RHome.CompetencyProfile, URL = "#" });
    ViewBag.Breadcrumb = list;
}
@section TopSlider{
    @Html.Partial("_sectionSlider")
}
<div id="kopa-posts-list-two-parts-6" class="widget kopa-article-list-widget-2 clearfix">
    <h3 class="widget-title"><span class="icon-title fa fa-diamond"></span>@ICB_Website.UI.Resources.RHome.CompetencyProfile</h3>
    <div>
        <iframe style="width:100%;height:1200px" src="@Model.Name"  seamless="seamless" scrolling="no" frameborder="0" allowtransparency="true" allowfullscreen="true" style="visibility: visible; min-height: 300px; height: 600px;"></iframe>
    </div>
    <div class="kopa-other-post" style="width:100%;">
        <h4 class="list-post-title" style="font-size:20px;margin-top:20px;">
            <span>@Model.Caption</span>
        </h4>
        <div>
           @Html.Raw(Model.Description)
        </div>
    </div>
</div>
